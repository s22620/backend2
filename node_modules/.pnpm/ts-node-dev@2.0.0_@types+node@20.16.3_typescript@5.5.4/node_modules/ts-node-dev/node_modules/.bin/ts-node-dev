#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alto64/Documents/Projects/be-hotel/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.16.3_typescript@5.5.4/node_modules/ts-node-dev/lib/node_modules:/home/alto64/Documents/Projects/be-hotel/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.16.3_typescript@5.5.4/node_modules/ts-node-dev/node_modules:/home/alto64/Documents/Projects/be-hotel/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.16.3_typescript@5.5.4/node_modules:/home/alto64/Documents/Projects/be-hotel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/alto64/Documents/Projects/be-hotel/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.16.3_typescript@5.5.4/node_modules/ts-node-dev/lib/node_modules:/home/alto64/Documents/Projects/be-hotel/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.16.3_typescript@5.5.4/node_modules/ts-node-dev/node_modules:/home/alto64/Documents/Projects/be-hotel/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.16.3_typescript@5.5.4/node_modules:/home/alto64/Documents/Projects/be-hotel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
